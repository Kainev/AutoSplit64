{
  "name": "autosplit64",
  "version": "1.0.0-alpha.1",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "concurrently \"pnpm run dev:react\" \"pnpm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "electron .",
    "build": "pnpm run build:as64 && pnpm run build:react && pnpm run build:electron",
    "build:react": "vite build",
    "build:electron": "electron-builder",
    "build:as64": "py\\python.exe tools\\as64.build.py",
    "serve:prod": "pnpm run build && pnpm run prod",
    "prod": "electron .",
    "clean": "rimraf dist"
  },
  "main": "as64ui/electron/main.js",
  "build": {
    "appId": "com.kainev.autosplit64",
    "productName": "AutoSplit64",
    "files": [
      "as64ui/dist/**/*",
      "as64ui/electron/**"
    ],
    "extraFiles": [
      {
        "from": "py",
        "to": "py",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "as64.dist/as64",
        "to": "as64"
      },
      {
        "from": "as64.dist/plugins",
        "to": "plugins",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "defaults.toml",
        "to": "defaults.toml"
      },
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "routes",
        "to": "routes",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "templates",
        "to": "templates",
        "filter": [
          "**/*"
        ]
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "include": "installer.nsh"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [],
  "author": "Kainev",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "electron-log": "^5.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "toml": "^3.0.0",
    "tomlify-j0.4": "^3.0.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
